# Database settings
DATABASE_DIALECT=postgresql
DATABASE_DRIVER=asyncpg
DATABASE_USER=user
DATABASE_PASSWORD=password
DATABASE_NAME=postgres-service-database
DATABASE_PORT=5556
DATABASE_PORT_NETWORK=5432
DATABASE_HOST=postgres-app-users

# Alchemy settings
DATABASE_ECHO=true
DATABASE_POOL_RECYCLE=3600
DATABASE_POOL_PRE_PING=true
DATABASE_AUTO_FLUSH=false
DATABASE_EXPIRE_ON_COMMIT=false

# FastAPI settings
API_PORT=8000
CORS_ALLOW_ORIGINS=["*"]
CORS_ALLOW_HEADERS=["*"]
CORS_ALLOW_CREDENTIALS=true
CORS_ALLOW_METHODS=["*"]
# PROMETHEUS_MULTIPROC_DIR=resources/prometheus
SERVER_BIND=0.0.0.0:8001
SERVER_TIMEOUT=120
SERVER_WORKER_CLASS=uvicorn.workers.UvicornWorker
SERVER_LOG_LEVEL=info
SERVER_LOG_FILE=-
SERVER_WORKERS=4

# Telegram Settings
